<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>cookie and session</title>
    </head>
    <body>
        <form action="/add" method="post">
            <h2>Add Cookie</h2>
            <label>Key: <input type="text" name="key"/></label>
            <label>Value: <input type="text" name="value"/></label>
            <input type="submit" value="add"/>
        </form>
        </br>
        <div>
            <table>
                <tr>
                    <th>Key</th>
                    <th>Value</th>
                </tr>
                <% for(let key in cookies){ %>
                    <tr>
                        <td><%= key%></td>
                        <td><%= cookies[key]%></td>
                    </tr>
                <%}
                %>
            </table>
        </div>
    </body>
</html>